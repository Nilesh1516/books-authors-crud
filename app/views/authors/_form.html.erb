<ol>
  <% @author.errors.full_messages.each do |message| %>
  <li><%= message %></li>
  <% end %>
</ol>


<%= form_for @author do |f| %>
  <%= f.label :First_Name %>
  <%= f.text_field :first_name %><br><br>

 <%= f.label :Last_Name %>
  <%= f.text_field :last_name %><br><br>

  <%= f.label :birth_year %>
  <%= f.number_field :birth_year %><br><br>

  <%= f.submit %>
<% end %>
